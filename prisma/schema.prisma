generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Keyword {
  id      Int      @id @default(autoincrement())
  keyword String   @unique
  related Json     // Python 결과 리스트를 JSON으로 저장
  createdAt DateTime @default(now())
}


